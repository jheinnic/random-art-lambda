{
  "javascript.preferences.quoteStyle": "double",
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": false,
  "javascript.format.insertSpaceAfterConstructor": true,
  "vsintellicode.features.apiExamplests": "enabled",
  "javascript.preferences.importModuleSpecifierEnding": "js",
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.format.semicolons": "remove",
  "typescriptHero.verbosity": "info",
  "typescriptHero.imports.stringQuoteStyle": "\"",
  "typescriptHero.imports.organizeOnSave": true,
  "typescriptHero.imports.organizeSortsByFirstSpecifier": true,
  "typescriptHero.imports.insertSemicolons": false,
  "typescript.workspaceSymbols.scope": "currentProject",
  "typescript.tsserver.useSyntaxServer": "always",
  "typescript.tsserver.maxTsServerMemory": 8192,
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "typescript.tsserver.enableTracing": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.preferences.quoteStyle": "double",
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "typescript.preferences.importModuleSpecifierEnding": "js",
  "typescript.locale": "en",
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.format.semicolons": "remove",
  "typescript.format.insertSpaceAfterTypeAssertion": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "typescript.format.insertSpaceAfterConstructor": true,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescriptHero.imports.disableImportRemovalOnOrganize": true,
  "typescriptHero.imports.removeTrailingIndex": false,
  "java.saveActions.organizeImports": false,
  "importSorter.generalConfiguration.sortOnBeforeSave": true,
  "importSorter.importStringConfiguration.hasSemicolon": false,
  "importSorter.importStringConfiguration.quoteMark": "double",
  "eslint.format.enable": true,
  "editor.formatOnType": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "[typescript]": {
    // "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "typescript.suggest.enabled": true
  },
  "cSpell.words": ["Blockstores", "Serdes"],
  "typescript.tsserver.watchOptions": {}
}
